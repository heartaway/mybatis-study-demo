<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mybatis.demo.basic.model.BlogMapper">
    <cache/>

    <select id="selectBlogFromXml" resultType="com.mybatis.demo.domain.BlogDO">
        select * from mybatis_demo_blog where id = #{id}
    </select>
</mapper>